net: "caffeproto/net.prototxt"
test_iter: 100
test_interval: 1000
test_compute_loss: true
# ---------- training parameters ----------
base_lr: 0.006
weight_decay: 0.0005
lr_policy: "fixed"
# ---------- visualization parameters ----------
display: 1000
max_iter: 10000
snapshot: 1000
snapshot_prefix: "caffeproto/snapshots/cae"
solver_mode: GPU