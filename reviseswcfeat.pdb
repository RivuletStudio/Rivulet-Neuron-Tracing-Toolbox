#!/bin/bash
# pbs launching script example for NAMD job
# PLEASE DO NOT CHANGE THIS FILE. TO SUBMIT QUEUE, PLS MAKE A COPY OF THIS FILE AND MAKE THE ACCORDING CHANGES

#     job name:
#PBS -N FishHead 
#PBS -P RDS-FEI-NRMMCI-RW
#PBS -q compute 

#     how many cpus?
#PBS -l ncpus=16
#PBS -l nodes=1
#PBS -M zdhpeter1991@gmail.com 

#PBS -l pmem=24gb

# How long to run the job? (hours:minutes:seconds)
#PBS -l walltime=3:00:0
#PBS -m ae
#     Name of output file:
#PBS -o reviseswc.txt

#     Environmental varibles to make it work:
 
module load matlab;
cd $PBS_O_WORKDIR;

#     Launching the job!
JOBNAME='ReviseswcVesselness';
DATETIME=$(date +"%F-%T");
echo "The job name is : $JOBNAME";
echo "Time now is : $DATETIME";
DATAPATH=/project/RDS-FEI-NRMMCI-RW/Data/OP/OP_Feat;
#     Run Script
matlab -nodesktop -nosplash -r "pwd";
